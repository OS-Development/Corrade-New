<?xml version="1.0" encoding="utf-16"?>
<SerializedNotification xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Notification>permission</Notification>
  <NotificationParameters>
    <Item>
      <Key>
        <string>OpenMetaverse.ScriptQuestionEventArgs</string>
      </Key>
      <Value>
        <ArrayOfParameter>
          <Parameter>
            <Path>ObjectOwnerName</Path>
            <Processing>
              <NameSplit>
                <First>firstname</First>
                <Last>lastname</Last>
              </NameSplit>
            </Processing>
          </Parameter>
          <Parameter>
            <Path>ObjectOwnerName</Path>
            <Name>owner</Name>
            <Processing>
              <Resolve>
                <ResolveType>AGENT</ResolveType>
                <Type>UUID</Type>
              </Resolve>
            </Processing>
          </Parameter>
          <Parameter>
            <Path>ObjectName</Path>
            <Name>name</Name>
          </Parameter>
          <Parameter>
            <Path>ItemID</Path>
            <Name>item</Name>
          </Parameter>
          <Parameter>
            <Path>TaskID</Path>
            <Name>task</Name>
          </Parameter>
          <Parameter>
            <Path>Questions</Path>
            <Name>permissions</Name>
          </Parameter>
          <Parameter>
            <Path>Simulator.Name</Path>
            <Name>region</Name>
          </Parameter>
        </ArrayOfParameter>
      </Value>
    </Item>
  </NotificationParameters>
</SerializedNotification>