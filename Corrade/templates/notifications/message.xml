<?xml version="1.0" encoding="utf-8"?>
<SerializedNotification xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Notification>message</Notification>
  <NotificationParameters>
    <Item>
      <Key>
        <string>OpenMetaverse.InstantMessageEventArgs</string>
      </Key>
      <Value>
        <ArrayOfParameter>
          <Parameter>
            <Path>IM.FromAgentName</Path>
            <Processing>
              <NameSplit>
                <First>firstname</First>
                <Last>lastname</Last>
              </NameSplit>
            </Processing>
          </Parameter>
          <Parameter>
            <Path>IM.FromAgentID</Path>
            <Name>agent</Name>
          </Parameter>
          <Parameter>
            <Path>IM.Message</Path>
            <Name>message</Name>
            <Processing>
              <IdentifyLanguage>
                <Name>language</Name>
              </IdentifyLanguage>
            </Processing>
          </Parameter>
          <Parameter>
            <Name>date</Name>
            <Processing>
              <Method>
                <Type>System.DateTime</Type>
                <Name>ToUniversalTime</Name>
                <Path>Now</Path>
              </Method>
            </Processing>
          </Parameter>
        </ArrayOfParameter>
      </Value>
    </Item>
  </NotificationParameters>
</SerializedNotification>
